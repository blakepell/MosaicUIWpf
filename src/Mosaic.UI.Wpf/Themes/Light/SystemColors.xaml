<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  SYSTEM COLORS  -->

    <!--  ActiveBorder  -->
    <SolidColorBrush x:Key="{x:Static SystemColors.ActiveBorderBrushKey}" Color="#FF0078D7" />
    <Color x:Key="{x:Static SystemColors.ActiveBorderColorKey}">#FF0078D7</Color>
    <!--  ActiveCaption  -->
    <SolidColorBrush x:Key="{x:Static SystemColors.ActiveCaptionBrushKey}" Color="#FF0078D7" />
