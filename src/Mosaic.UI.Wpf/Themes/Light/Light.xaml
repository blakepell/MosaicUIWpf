    <SolidColorBrush x:Key="{x:Static themes:MosaicTheme.SidebarSelectedBackgroundBrush}" Color="{DynamicResource {x:Static themes:MosaicTheme.SidebarSelectedBackgroundColor}}" />

    <!--  Button  -->
    <Color x:Key="Button.Static.BackgroundColor">#FFDDDDDD</Color>
    <SolidColorBrush x:Key="Button.Static.Background" Color="{DynamicResource Button.Static.BackgroundColor}" />

    <Color x:Key="Button.Static.BorderColor">#FF707070</Color>
    <SolidColorBrush x:Key="Button.Static.Border" Color="{DynamicResource Button.Static.BorderColor}" />

    <Color x:Key="Button.MouseOver.BackgroundColor">#FFBEE6FD</Color>
