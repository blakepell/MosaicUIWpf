    <Color x:Key="{x:Static themes:MosaicTheme.ControlHoverBackgroundColor}">#E2E2E2</Color>
    <SolidColorBrush x:Key="{x:Static themes:MosaicTheme.ControlHoverBackgroundBrush}" Color="{DynamicResource {x:Static themes:MosaicTheme.ControlHoverBackgroundColor}}" />

