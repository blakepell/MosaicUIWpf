                        <Rectangle
                            x:Name="B1"
                            Height="1"
                            DockPanel.Dock="Bottom"
                            Fill="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}"
                            Visibility="Hidden" />
                        <Rectangle
                            x:Name="L1"
                            Width="1"
                            DockPanel.Dock="Left"
                            Fill="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}"
                            Visibility="Hidden" />
                        <Rectangle
                            x:Name="T1"
                            Height="1"
                            DockPanel.Dock="Top"
                            Fill="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}"
                            Visibility="Hidden" />
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
