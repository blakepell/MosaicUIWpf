                                                        <GradientStop Offset="0" Color="#B3E2A2" />
                                                        <GradientStop Offset="1" Color="#25B40E" />
                                                    </GradientStopCollection>
                                                </GradientBrush.GradientStops>
                                            </LinearGradientBrush>
